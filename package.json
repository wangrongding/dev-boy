    "chalk": "^5.1.2",
    "commander": "^9.4.1",
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^5.0.1",
    "@types/inquirer": "^9.0.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "rollup": "^3.2.3",
    "rollup-plugin-delete": "^2.0.0",
